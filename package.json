{
  "name": "react-transform-boilerplate",
  "version": "1.0.0",
  "description": "A new Webpack boilerplate with hot reloading React components, and error handling on module and component level.",
  "scripts": {
    "clean": "rimraf dist",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack",
    "start": "node devServer.js",
    "lint:client": "eslint client-src; exit 0",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "test": "NODE_ENV=production mocha --compilers js:babel-core/register --require ./test/test_helper.js 'test/**/*.@(js|jsx)'",
    "test:watch": "npm run test -- --watch",
    "babel:build": "babel --watch=./server-src --out-dir=./build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gaearon/react-transform-boilerplate.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack",
    "babel",
    "react-transform"
  ],
  "author": "Neil Gordon",
  "license": "CC0-1.0",
  "homepage": "https://github.com/neil-g/App_Boilerplate",
  "devDependencies": {
    "babel-core": "^6.6.5",
    "babel-eslint": "^5.0.0-beta4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.1.1",
    "chai": "^3.5.0",
    "chai-immutable": "^1.6.0",
    "cross-env": "^1.0.7",
    "css-loader": "^0.24.0",
    "enzyme": "^2.4.1",
    "eslint": "^1.10.3",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-react": "^3.11.3",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.13.3",
    "flow-bin": "^0.31.1",
    "jsdom": "^9.4.2",
    "mocha": "^3.0.2",
    "node-sass": "^3.8.0",
    "react-addons-test-utils": "^15.3.1",
    "redux-devtools": "^3.3.1",
    "redux-logger": "^2.6.1",
    "rimraf": "^2.4.3",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.9.1"
  },
  "dependencies": {
    "bluebird": "^3.4.2",
    "body-parser": "^1.15.2",
    "cookie-parser": "^1.4.3",
    "es6-promise": "^3.2.1",
    "express-graphql": "^0.5.3",
    "graphql": "^0.6.2",
    "immutable": "^3.8.1",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.15.0",
    "moment": "^2.14.1",
    "morgan": "^1.7.0",
    "pg": "^6.1.0",
    "pg-hstore": "^2.3.2",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-redux": "^4.4.5",
    "react-router": "^2.7.0",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "sequelize": "^3.24.1",
    "serve-favicon": "^2.3.0",
    "skeleton-css": "^2.0.4"
  }
}
